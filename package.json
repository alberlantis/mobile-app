{
  "name": "satlantis",
  "version": "0.14.3",
  "main": "index.ts",
  "license": "UNLICENSED",
  "repository": "git@github.com:satlantis-dev/mobile-app.git",
  "scripts": {
    "ios:submit": "yarn eas:config:prod && APP_ENV=prod eas submit --platform ios --latest",
    "ios:prod": "yarn prebuild:prod && APP_ENV=prod expo run:ios",
    "ios:prod:release": "yarn prebuild:prod && APP_ENV=prod expo run:ios --configuration Release",
    "ios": "yarn prebuild:dev && APP_ENV=dev expo run:ios",
    "ios:release": "yarn prebuild:dev && APP_ENV=dev expo run:ios --configuration Release",
    "ios:build:dev": "yarn eas:config:dev && eas build --profile development --platform ios",
    "ios:build:prod": "yarn eas:config:prod && eas build --profile production --platform ios",
    "ios:build:release": "yarn eas:config:prod && eas build --profile release --platform ios",
    "android:submit": "yarn eas:config:prod && APP_ENV=prod eas submit --platform android --latest",
    "android": "yarn prebuild:dev && APP_ENV=dev expo run:android",
    "android:release": "yarn prebuild:dev && APP_ENV=dev expo run:android --variant=release",
    "android:prod": "yarn prebuild:prod && APP_ENV=prod expo run:android",
    "android:prod:release": "yarn prebuild:prod && APP_ENV=prod expo run:android --variant=release",
    "android:build:dev": "yarn eas:config:dev && eas build --profile development --platform android",
    "android:build:prod": "yarn eas:config:prod && eas build --profile production --platform android",
    "android:build:release": "yarn eas:config:prod && eas build --profile release --platform android",
    "start:dev": "APP_ENV=dev npx expo start --reset-cache",
    "start:prod": "APP_ENV=prod npx expo start --reset-cache",
    "eas:config:dev": "APP_ENV=dev gen-eas-config",
    "eas:config:prod": "APP_ENV=prod gen-eas-config",
    "expo:publish": "VERSION=$(node -p \"require('./package.json').version\") && APP_ENV=dev eas update --message=\"app-version: ${VERSION}\" --clear-cache --non-interactive",
    "expo:update:dev": "VERSION=$(node -p \"require('./package.json').version\") && APP_ENV=dev eas update --message=\"app-version: ${VERSION}\" --channel=development --clear-cache --non-interactive",
    "expo:update:prod": "VERSION=$(node -p \"require('./package.json').version\") && APP_ENV=prod eas update --message=\"app-version: ${VERSION}\" --channel=production --clear-cache --non-interactive",
    "expo:update:release": "VERSION=$(node -p \"require('./package.json').version\") && APP_ENV=prod eas update --message=\"app-version: ${VERSION}\" --channel=release --clear-cache --non-interactive",
    "prebuild:dev": "CI=1 APP_ENV=dev expo prebuild --clean",
    "prebuild:prod": "CI=1 APP_ENV=prod expo prebuild --clean",
    "check:type": "tsc",
    "check:lint": "eslint src/ --ext .ts,.tsx",
    "check:fix": "yarn check:lint --fix",
    "check:project": "expo-doctor",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky",
    "clean:modules": "rm -rf node_modules && yarn --clear-cache",
    "clean:watchman": "watchman watch-del-all",
    "clean:install": "yarn clean:modules && yarn clean:watchman",
    "bump:package:patch": "yarn version --no-git-tag-version --patch",
    "bump:package:minor": "yarn version --no-git-tag-version --minor",
    "bump:package:major": "yarn version --no-git-tag-version --major",
    "increment:version:patch:dev": "./increment_version.sh patch dev",
    "increment:version:minor:dev": "./increment_version.sh minor dev",
    "increment:version:major:dev": "./increment_version.sh major dev",
    "increment:version:patch:prod": "./increment_version.sh patch prod",
    "increment:version:minor:prod": "./increment_version.sh minor prod",
    "increment:version:major:prod": "./increment_version.sh major prod"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@expo/metro-config": "~0.18.11",
    "@testing-library/react-native": "^12.5.2",
    "@types/jest": "^29.5.12",
    "@types/json-stable-stringify": "^1.0.36",
    "@types/react": "~18.2.79",
    "@types/react-test-renderer": "^18.3.0",
    "babel-preset-expo": "^11.0.13",
    "eas-config-ts": "^1.0.5",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "expo-doctor": "^1.9.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "react-native-eject": "^0.2.0",
    "react-test-renderer": "18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@blowater/nostr-sdk": "npm:@jsr/blowater__nostr-sdk@^0.0.16",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@satlantis/api-client": "npm:@jsr/satlantis__api-client@^0.0.39",
    "expo": "~51.0.31",
    "expo-application": "~5.9.1",
    "expo-asset": "~10.0.10",
    "expo-camera": "~15.0.15",
    "expo-clipboard": "~6.0.3",
    "expo-constants": "~16.0.2",
    "expo-image-picker": "~15.0.7",
    "expo-secure-store": "^13.0.2",
    "expo-splash-screen": "~0.27.5",
    "expo-updates": "~0.25.24",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-get-random-values": "^1.11.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0",
    "redux-persist-expo-securestore": "^2.0.0",
    "text-encoding-polyfill": "^0.6.7"
  }
}
