{
  "name": "satlantis",
  "version": "0.0.1",
  "main": "index.ts",
  "license": "UNLICENSED",
  "repository": "git@github.com:satlantis-dev/mobile-app.git",
  "engines": {
    "node": "20.9.0"
  },
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "npx expo start --reset-cache",
    "expo:start": "expo start --tuner",
    "expo:go": "eas update --auto",
    "build:ios:dev": "eas build --platform ios --profile development",
    "build:android:dev": "eas build --platform android --profile development",
    "check:type": "tsc",
    "check:lint": "eslint src/ --ext .ts,.tsx",
    "check:fix": "yarn check:lint --fix",
    "check:project": "expo-doctor",
    "test": "jest",
    "prepare": "husky",
    "clean:modules": "rm -rf node_modules && yarn --clear-cache",
    "clean:watchman": "watchman watch-del-all",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && rm -rf build Podfile.lock Pods && pod install --repo-update && cd ..",
    "clean:install": "yarn clean:modules && yarn clean:android && yarn clean:ios && yarn clean:watchman"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@expo/metro-config": "~0.18.11",
    "@testing-library/react-native": "^12.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.79",
    "@types/react-test-renderer": "^18.3.0",
    "babel-preset-expo": "^11.0.13",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "expo-doctor": "^1.9.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "react-native-eject": "^0.2.0",
    "react-test-renderer": "^18.3.1",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "expo": "~51.0.26",
    "expo-updates": "~0.25.22",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1"
  }
}
